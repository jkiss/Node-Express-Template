    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "ejs": "^3.0.1",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "got": "^10.6.0",
    "lodash": "^4.17.14",
    "log4js": "^6.1.2",
    "moment": "^2.24.0",
    "mongoose": "^5.9.4",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "nodemailer": "^6.4.5",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.0.1",
